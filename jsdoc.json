{
	"plugins": ["plugins/markdown"],
	"source": {
		"include": ["./source/horoscope/js/"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(node_modules/|docs)"
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"destination": "./docs",
		"recurse": true,
		"readme": "./README.md"
	}
}